---
import type { TextBlock } from "../api/lib/notion-types";
import NotionBlockItemText from "./NotionBlockItemText.astro";

export interface Props {
	data: Required<TextBlock>["properties"]["title"];
}

const { data = [] } = Astro.props as Props;
---

<Fragment>
	{data.map((textItem) => (
		<NotionBlockItemText data={textItem} />
	))}
</Fragment>